packadd! dracula
syntax enable
colorscheme dracula
set nocompatible
filetype on
filetype plugin on
filetype indent on
set number
set nowrap
set showmode
set hlsearch
set history=1000
set showcmd
set nobackup
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx
